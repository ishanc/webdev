<script type="text/javascript">
window.onload = function() {
$(function() {
if (typeof Storage != "undefined") {

if (localStorage.getItem("ME_98point6_Popup")==_satellite.getVar("adp_user_profile_wm-IdentificationNumber")) {
console.log("Converted audience");}
	else {
if (sessionStorage.getItem("ME_98point6_Popup")==_satellite.getVar("adp_user_profile_wm-IdentificationNumber")) {
console.log("Same session returning audience");}
else {
setTimeout(function() {
$('#myModal').modal({show: true});
}, 1400);


sessionStorage.setItem("ME_98point6_Popup", _satellite.getVar("adp_user_profile_wm-IdentificationNumber"));
}
	}
}});
}
function converted () {
	localStorage.setItem("ME_98point6_Popup", _satellite.getVar("adp_user_profile_wm-IdentificationNumber"));
  
setTimeout(function() {
$('#myModal').modal('hide');
}, 1400);
  
}
</script>